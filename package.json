{
  "name": "schood",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.6.4",
    "bootstrap-switch": "^3.4.0",
    "enzyme": "^3.11.0",
    "eslint-plugin-cypress": "^2.15.1",
    "history": "^5.3.0",
    "moment": "^2.30.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-native-web": "^0.19.10",
    "react-navigation": "^5.0.0",
    "react-router-dom": "^6.21.1",
    "react-scripts": "5.0.1",
    "react-switch-input": "^1.1.0",
    "reactjs-popup": "^2.0.6",
    "sass": "^1.66.1",
    "standard": "^17.1.0",
    "styled-components": "^6.1.6",
    "web-vitals": "^3.4.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "coveragePathIgnorePatterns": [
      "src/index.js"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage --watchAll=false --runInBand",
    "linter": "standard --fix --global it --global expect --global describe --global beforeAll --global afterEach --global beforeEach --global afterAll --global test --global jest --global localStorage --global cy --global sessionStorage --global File"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/user-event": "^14.5.2",
    "cypress": "^13.6.2",
    "fetch-mock": "^9.11.0",
    "fetch-mock-jest": "^1.5.1",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-watch-typeahead": "^2.2.2",
    "msw": "^2.0.12"
  }
}
