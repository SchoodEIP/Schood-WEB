export default async function mockFetch(url) {
    console.log('here the url is ', url)
    switch(url) {
        case "http://localhost:8080/user/forgottenPassword": {
            return {
                ok: true,
                status: 200,
                json: async () => {},
            };
        }
        default: {
            throw new Error(`Unhandled request: ${url}`);
        }
    }
}